fn do_nothing() {
    print "Doing nothing"
}

fn signal() -> void {
    return
}

fn main() -> num {
    var ptr = adr str // Expected: None
    
    // Check if we can assign later
    ptr = ref "Now I exist"
    
    print deref ptr
    
    do_nothing()
    signal()
    
    var v_ptr = adr void
    
    // Assignment compatibility (Address extraction)
    v_ptr = ptr
    print v_ptr // Should print address (number)
    
    return 0
}

print main()
