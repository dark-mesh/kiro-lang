import std_fs

fn main() {
    print "Testing multi-error..."
    var res = std_fs.read("non_existent_file_xyz")
    
    // Test: Should skip PermissionDenied and match NotFound
    on (res) {
        print "Should not be here"
    } error PermissionDenied {
        print "Caught PermissionDenied (Unexpected)" 
    } error NotFound {
        print "Caught NotFound (Expected)"
    } error {
        print "Caught generic (Unexpected)"
    }

    print "Done"
}
main()
